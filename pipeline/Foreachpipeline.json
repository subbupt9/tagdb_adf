								"timeout": "0.00:15:00",
								"retry": 2,
								"retryIntervalInSeconds": 3000,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
