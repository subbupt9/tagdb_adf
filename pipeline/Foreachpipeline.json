									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"name": "Sno",
												"type": "Int32",
												"physicalType": "int"
											},
											"sink": {
												"name": "Sno",
												"type": "Int32",
												"physicalType": "int"
											}
										},
										{
											"source": {
												"name": "StoreName",
